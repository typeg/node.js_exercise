<html>
  <head>
    <% include ./header.ejs %>
  </head>
  <body>
    <h1><a href='/'><%=title%></a></h1>
      <table border=0>
      <tr>
	<td>Name</td>
	<td>Description</td>
	<td></td>
      </tr>
      <%
	for(var i=0; i<myList.length; i++){
      %>
      <tr>
	<td><%=myList[i].name%></td>
	<td><%=myList[i].description%></td>
	<td>
	<input type='button' value='EDIT' onclick="updateList('<%=myList[i].idxno%>')" />
	<input type='button' value='DEL' onclick="deleteList('<%=myList[i].idxno%>')" />
	</td>
      </tr>
      <%}%>
      <tr>
	<td colspan=3>
	<input type='button' value='CREATE' onclick='javascript:createList();'>
	</td>
      </tr>
    </table>
  </body>
</html>
